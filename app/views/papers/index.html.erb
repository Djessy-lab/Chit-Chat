<%= render "shared/navbar" %>
<h1>Mes documents</h1>
<% @papers.each do |paper| %>
  <%= link_to paper.name, paper_path(paper) %>
  <%= link_to '<i class="fa-regular fa-trash-can"></i>'.html_safe, paper_path(paper), data: { turbo_method: :delete, turbo_confirm: "Êtes vous sûr ?" } %>
<% end %>
<%= link_to "Ajouter un document", new_paper_path, class: "button" %>
<%= render "shared/navmenu" %>
